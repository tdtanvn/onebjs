{
  "name": "@oneb-sdk/client",
  "version": "1.0.0",
  "description": "",
  "main": "./dist-cjs/index.js",
  "types": "./dist-types/index.d.ts",
  "module": "./dist-es/index.js",
  "files": [
    "dist-*"
  ],
  "scripts": {
    "build": "concurrently 'yarn:build:cjs' 'yarn:build:es' 'yarn:build:types'",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build:types": "tsc -p tsconfig.types.json",
    "lint": "eslint ./*.ts ./lib/**/*.ts",
    "build:proto": "yarn build:protojs && yarn build:protod",
    "build:protojs": "pbjs -t static-module -w commonjs --keep-case -o ./src/proto/proto.js  ./proto/ProtoMessage.proto",
    "build:protod": "pbts -o ./src/proto/proto.d.ts ./src/proto/proto.js"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "oneb.tech",
    "game",
    "backend",
    "service"
  ],
  "author": "oneb.tech",
  "devDependencies": {
    "@types/node-fetch": "^2.6.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "protobufjs-cli": "^1.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "node-fetch": "^2.6.7",
    "protobufjs": "^7.2.3"
  }
}
